    "import sys\n",
    "\n",
    "N = 0\n",
    "try:\n",
    "    N = int(input())\n",
    "except ValueError:\n",
    "    print('Invalid Number')\n",
    "    \n",
    "for i in range(N):\n",
    "    s = input()\n",
    "    result = \"\"\n",
    "    for a in range(1,len(s) + 1):\n",
    "        for b in range(0,len(s) - a + 1):\n",
    "            result += s[b:b+a] + \" \"\n",
    "    print(result[:-1])"
   ]
